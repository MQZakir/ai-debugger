{
  "name": "ender-debugger",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:build": "tsc -p electron/tsconfig.json && vite build",
    "electron:dev": "tsc -p electron/tsconfig.json && cross-env NODE_ENV=development electron .",
    "start-backend": "node backend/server.js",
    "start:frontend": "concurrently \"npm run dev\" \"npm run electron:dev\"",
    "start:backend:file": "py backend/src/websocket_server.py",
    "start:backend:api": "cd src/server && py api.py",
    "start:server": "node src/server/systemMetrics.js",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend:file\" \"npm run start:server\" \"npm run start:backend:api\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "file-server": "node file-server.js",
    "test-multi-runtime": "node tests/multi-runtime-test.js",
    "test-single-runtime": "node tests/single-runtime-test.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@types/d3": "^7.4.3",
    "@types/os-utils": "^0.0.4",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "electron": "^28.1.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "os-utils": "^0.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.21.1",
    "styled-components": "^6.1.17",
    "supabase": "^2.22.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.0.8"
  }
}
